{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/default/index.tsx",
        "style": "templates/default/TemplateName.module.scss"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "page": {
      "customTemplates": {
        "component": "templates/default/index.tsx",
        "style": "templates/default/TemplateName.module.scss"
      },
      "path": "src/pages",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "page-component": {
      "customTemplates": {
        "component": "templates/page-component/index.tsx",
        "style": "templates/page-component/TemplateName.module.scss"
      },
      "path": "src/pages",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "component": {
      "customTemplates": {
        "component": "templates/component/TemplateName.tsx"
      },
      "path": "src/pages",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "redux": {
      "customTemplates": {
        "slice": "templates/redux/slice.ts",
        "types": "templates/redux/types.ts"
      },
      "path": "src/redux",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "withSlice": true,
      "withTypes": true
    }
  }
}
